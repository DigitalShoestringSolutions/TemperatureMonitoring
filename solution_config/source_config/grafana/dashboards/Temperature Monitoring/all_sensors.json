      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb"
          },
          "hide": false,
          "query": "import \"date\"\r\n\r\nfrom(bucket: \"temperature_monitoring\")\r\n  |> range(start: date.sub(d:2h,from:v.timeRangeStart), stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"temperature_reading\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"AlertVal\" or r[\"_field\"] == \"temperature\")\r\n  |> group(columns: [\"machine\",\"_field\"])\r\n  |> last()\r\n  \r\n  |> map( fn: (r) => ({ r with Name: r.machine }) )                        // convert machine name from a tag to part of the result, so it survives pivot\r\n  |> keep(columns: [\"_time\", \"Name\", \"_field\", \"_value\"])                  // discard original machine tag (and everything else not needed)\r\n\r\n  |> pivot(rowKey: [\"Name\"], columnKey: [\"_field\"], valueColumn: \"_value\") // put AlertVal and temperature on the same rows, matching by (last reading for) Name (aka machine name)\r\n\r\n  |> map(                                                                  // translate AlertVal ints into colours\r\n      fn: (r) => ({ r with color: \r\n          if      r.AlertVal == -1 then \"blue\"   // \"#1f60c4\" \r\n          else if r.AlertVal == 0  then \"green\"  // \"#37872d\"\r\n          else if r.AlertVal == 1  then \"red\"    // \"#c4162a\"  \r\n          else                          \"yellow\" // unknown or error\r\n      }))\r\n\r\n  |> keep(columns: [\"Name\", \"color\", \"temperature\"])    // discard AlertVal\r\n",
          "refId": "A: Pivot"
        }
      ],
      "title": "Temperature Value",
      "transformations": [
        {
          "id": "rowsToFields",
          "options": {
            "mappings": [
              {
                "fieldName": "color",
                "handlerKey": "color"
              },
              {
                "fieldName": "temperature",
                "handlerKey": "field.value"
              },
              {
                "fieldName": "Name",
                "handlerKey": "field.name"
              }
            ]
          }
        }
      ],
      "type": "stat"
    }
